{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "jest": true
    },
    "globals": {
      "web3": true,
      "assert": true,
      "contract": true,
      "artifacts": true
    },
    "rules": {
        "quotes": ["error", "double"],
        "object-curly-newline": 0,
        "react/jsx-one-expression-per-line": 0
    }
}
